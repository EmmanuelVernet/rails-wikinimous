<%= render "navbar" %>
<div class="container">
<div class="col-sm-8 offset-sm-2">
<h1>All articles</h1>
    <% @articles.each do |article|%>
    <article class="mb-3">
    <h2><%= link_to article.title, article_path(article) %></h2>
    <p><%= article.content %></p>
  </article>
  <% end %>
  <%= link_to "New article", new_article_path %>
  </div>
</div>